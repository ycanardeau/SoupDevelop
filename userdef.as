#module

	#uselib "msimg32"
	#func GradientFill "GradientFill" sptr, sptr, sptr, sptr, sptr, sptr

	#undef gradf
	#deffunc gradf int p1, int p2, int p3, int p4, int p5, int p6, int p7, int p8
		r = MAKEWORD(0, GetRValue(p7)), MAKEWORD(0, GetRValue(p8))
		g = MAKEWORD(0, GetGValue(p7)), MAKEWORD(0, GetGValue(p8))
		b = MAKEWORD(0, GetBValue(p7)), MAKEWORD(0, GetBValue(p8))
		redGreen = MAKELONG(r(0), g(0)), MAKELONG(r(1), g(1))
		blueAlpha = MAKELONG(b(0), 0), MAKELONG(b(1), 0)
		trivertex(0) = p2, p3
		trivertex(2) = redGreen(0), blueAlpha(0)
		trivertex(4) = p2 + p4, p3 + p5
		trivertex(6) = redGreen(1), blueAlpha(1)
		mesh = 0, 1
		GradientFill p1, varptr(trivertex), 2, varptr(mesh), 1, p6
		//redraw 1
		return

